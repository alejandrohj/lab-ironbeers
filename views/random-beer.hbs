{{#each randomBeer}}
    
<div class="randomBeer">
    <img class ="beerImg" src="{{this.image_url}}">
    <div class="beerDescription">
        {{> randomBeerTemplate this}}
        <h3>Food Pairing</h3>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    <p><b>Brewer Tips</b>{{this.brewers_tips}}</p>
    </div>
</div>
{{/each }}

